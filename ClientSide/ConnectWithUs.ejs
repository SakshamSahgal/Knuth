<!DOCTYPE html>
<html lang="en">
  <%- include('./ReusableComponents/BootstrapCDN.ejs') %>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Connect With US</title>
</head>
<body style="font-family: 'Helvetica', sans-serif; background-color: #f9f9f9; color: #333333;">

    <%- include('./ReusableComponents/navbar.ejs',{ activePage : page, email: emailTo }) %>

    <br><br>

    <section class="contact-section" style="padding: 50px 0;">
      <div class="container">
        <h2 class="contact-heading text-center" style="font-size: 36px; margin-bottom: 40px;">Connect with Us</h2>
        <div class="row justify-content-center">
          <div class="col-md-4 col-sm-6 mb-4">
            <div class="text-center">
              <div class="contact-icon" style="font-size: 48px; color: #3498db;">
                <i class="fab fa-telegram-plane"></i>
              </div>
              <div class="contact-label" style="font-size: 18px; font-weight: bold; margin-bottom: 10px;">Telegram</div>
              <a href="https://t.me/your_telegram" target="_blank" class="contact-link" style="color: #3498db; text-decoration: none;">Join Us</a>
            </div>
          </div>
          <div class="col-md-4 col-sm-6 mb-4">
            <div class="text-center">
              <div class="contact-icon" style="font-size: 48px; color: #27ae60;">
                <i class="fab fa-whatsapp"></i>
              </div>
              <div class="contact-label" style="font-size: 18px; font-weight: bold; margin-bottom: 10px;">WhatsApp</div>
              <a href="https://api.whatsapp.com/send?phone=your_whatsapp_number" target="_blank" class="contact-link" style="color: #27ae60; text-decoration: none;">Chat Now</a>
            </div>
          </div>
          <div class="col-md-4 col-sm-6 mb-4">
            <div class="text-center">
              <div class="contact-icon" style="font-size: 48px; color: #9b59b6;">
                <i class="fab fa-discord"></i>
              </div>
              <div class="contact-label" style="font-size: 18px; font-weight: bold; margin-bottom: 10px;">Discord</div>
              <a href="https://discord.gg/your_discord" target="_blank" class="contact-link" style="color: #9b59b6; text-decoration: none;">Join Server</a>
            </div>
          </div>
          <div class="col-md-4 col-sm-6 mb-4">
            <div class="text-center">
              <div class="contact-icon" style="font-size: 48px; color: #333;">
                <i class="fab fa-github"></i>
              </div>
              <div class="contact-label" style="font-size: 18px; font-weight: bold; margin-bottom: 10px;">GitHub</div>
              <a href="https://github.com/your_github" target="_blank" class="contact-link" style="color: #333; text-decoration: none;">View Profile</a>
            </div>
          </div>
          <div class="col-md-4 col-sm-6 mb-4">
            <div class="text-center">
              <div class="contact-icon" style="font-size: 48px; color: #e74c3c;">
                <i class="fab fa-linkedin-in"></i>
              </div>
              <div class="contact-label" style="font-size: 18px; font-weight: bold; margin-bottom: 10px;">LinkedIn</div>
              <a href="https://www.linkedin.com/in/your_linkedin" target="_blank" class="contact-link" style="color: #e74c3c; text-decoration: none;">Connect</a>
            </div>
          </div>
        </div>
      </div>
    </section>

  </body>
  <!-- //axios CDN -->
  <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
  <script src="http://localhost:3000/Scripts/Coordinators.js"></script>
</html>